TextButton.MouseButton1Down:connect(function()
	getgenv().autodroop = true;



	Spawn(function()
		while autodroop == true do 



			local args = {
				[1] = "DropMoney",
				[2] = "10000"
			}

			game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))

			wait(3)
		end
	end)
end)

